{
    "manifest_version": 2,
    "name": "Stack exchange styles",
    "version": "0.2",
    "content_scripts": [{
        "matches": ["*://meta.stackexchange.com/*", "*://stackoverflow.com/*", "*://*.stackexchange.com/*", "*://askubuntu.com/*", "*://serverfault.com/*", "*://superuser.com/*"],
        "css": ["style.css"]
    }]
}
